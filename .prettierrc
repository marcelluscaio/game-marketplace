{
	"printWidth": 90,
	"useTabs": true,
	"tabWidth": 2,
	"singleQuote": false,
	"bracketSpacing": true,
	"proseWrap": "preserve",
	"bracketSameLine": false,
	"singleAttributePerLine": true,
	"semi": true,
	"jsx-single-quote": false,
	"quoteProps": "as-needed",
	"arrowParens": "always",
	"trailingComma": "es5",
	"htmlWhitespaceSensitivity": "css"
}
